    "@azure/msal-node": "^1.0.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "connect-multiparty": "^2.2.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.2.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.18",
    "nodemailer-express-handlebars": "^4.0.0",
    "npm": "^7.9.0",
    "prompt-sync": "^4.2.0",
    "react-router-bootstrap": "^0.25.0",
    "read-excel-file": "^5.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
